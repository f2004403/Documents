package edu.stanford.cvkkumar.leetcode.linkedlist;

public class ListNode {
	
	int val;
	ListNode next;
	
	ListNode(int x){
		val = x;
		next = null;
	}
	
	public static String display(ListNode head) {
		StringBuffer sb = new StringBuffer();
		while (head != null) {
			sb.append(head.val + "->");
			
		}
	}
}
